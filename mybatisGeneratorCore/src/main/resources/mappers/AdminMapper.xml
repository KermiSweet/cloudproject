<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.generate.dao.AdminMapper">
  <resultMap id="BaseResultMap" type="com.generate.pojo.Admin">
    <result column="aname" jdbcType="VARCHAR" property="aname" />
    <result column="apwd" jdbcType="VARCHAR" property="apwd" />
  </resultMap>
  <insert id="insert" parameterType="com.generate.pojo.Admin">
    insert into admintable (aname, apwd)
    values (#{aname,jdbcType=VARCHAR}, #{apwd,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.generate.pojo.Admin">
    insert into admintable
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="aname != null">
        aname,
      </if>
      <if test="apwd != null">
        apwd,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="aname != null">
        #{aname,jdbcType=VARCHAR},
      </if>
      <if test="apwd != null">
        #{apwd,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>